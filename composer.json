{
    "name": "glicer/spell-checker",
    "type": "library",
    "description": "Spell check html files",
    "keywords": ["spell", "check", "html"],
    "homepage": "https://github.com/emmanuelroecker/php-spellchecker",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel ROECKER",
            "homepage": "http://dev.glicer.com"
        },
        {
            "name": "Rym BOUCHAGOUR",
            "homepage": "http://dev.glicer.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "~5.2",
        "glicer/simply-html": "~1.0",
        "symfony/finder": "~2.3",
        "symfony/process": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "GlSpellChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GlSpellChecker\\Tests\\": "tests/"
        }
    }
}